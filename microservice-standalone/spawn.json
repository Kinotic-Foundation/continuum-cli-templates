{
  "inherits": "../microservice-common",
  "globals": {
    "groovyVersion": "3.0.9",
    "springBootVersion": "2.7.4",
    "springDependencyManagementVersion": "1.0.14.RELEASE",
    "continuumVersion": "1.1.0"
  },
  "propertySchema":{
    "groupId": {
      "type": "string",
      "description": "Maven Group Id"
    },
    "name": {
      "type": "string",
      "description": "Application Name",
      "default": "{{artifactId | camelCase | upperFirst}}"
    },
    "javaVersion": {
      "type": "string",
      "enum": ["8", "11", "17"],
      "default": "17",
      "description": "Java source compatibility version"
    }
  }
}
