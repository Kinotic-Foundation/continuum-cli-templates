{
  "globals": {
    "continuumVersion": "1.2.0",
    "groovyVersion": "3.0.10",
    "lombokPluginVersion": "6.6.1",
    "springBootVersion": "2.7.9",
    "springDependencyManagementVersion": "1.1.0"
  },
  "propertySchema":{
    "projectName": {
      "type": "string",
      "description": "Project Name"
    },
    "groupId": {
      "type": "string",
      "description": "Maven Group Id"
    },
    "javaVersion": {
      "type": "string",
      "enum": ["11"],
      "default": "11",
      "description": "Java source compatibility version"
    }
  }
}
